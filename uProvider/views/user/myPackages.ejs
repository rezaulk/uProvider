                      <%= packageList[i].speed %>
                               <br>UNLIMITED<br>
                    <%= packageList[i].price %>
                    <br>
                    <form method="post">
                      <input type="submit" name="packageid" value="Activate">
                    </form>
                </div>

           <% } %>
   
    </div>
</div>
<div style="height: 90px;margin: 10px;background-color: #2C3A49">
           <div></div>

        </div>

